<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_remote__control"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0B0B0B"
    tools:context="com.example.george.naoapplication.Remote_Control">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="78dp"
        android:backgroundTint="#111111"
        android:tint="#EC5C2D"
        app:srcCompat="@drawable/ic_keyboard_arrow_up_black_24dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#111111"
        app:srcCompat="@drawable/ic_keyboard_arrow_left_black_24dp"
        android:id="@+id/imageButton3"
        android:tint="#EC5C2D"
        android:layout_alignTop="@+id/imageButton"
        android:layout_toLeftOf="@+id/imageButton"
        android:layout_toStartOf="@+id/imageButton"
        android:layout_marginTop="38dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#111111"
        android:tint="#EC5C2D"
        app:srcCompat="@drawable/ic_keyboard_arrow_right_black_24dp"
        android:id="@+id/imageButton4"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_toRightOf="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton" />

    <ImageButton
        android:layout_width="wrap_content"
        android:backgroundTint="#111111"
        android:layout_height="wrap_content"
        android:tint="#EC5C2D"
        app:srcCompat="@drawable/ic_keyboard_arrow_down_black_24dp"
        android:id="@+id/imageButton5"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton3"
        android:layout_toEndOf="@+id/imageButton3" />




    <Button
        android:text="Sit Relax"
        android:textColor="#E88137"
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button3"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Stand"
        android:textColor="#E88137"
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button9"
        android:layout_below="@+id/button3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/imageButton3"
        android:layout_alignRight="@+id/button3"
        android:layout_alignEnd="@+id/button3" />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/button3" />

    <Button
        android:textColor="#E88137"
        android:text="Belly"
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button6"
        android:layout_alignBaseline="@+id/button2"
        android:layout_alignBottom="@+id/button2"
        android:layout_toRightOf="@+id/button2" />

    <Button
        android:textColor="#E88137"
        android:text="Init"
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button4"
        android:layout_below="@+id/button6"
        android:layout_toEndOf="@+id/button2"
        android:layout_alignRight="@+id/button6"
        android:layout_toRightOf="@+id/button2" />

    <ImageButton
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="#EC5C2D"
        app:srcCompat="@drawable/camera"
        android:id="@+id/imageButton2"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:layout_below="@+id/button8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/button4"
        android:layout_toRightOf="@+id/button4"
        android:layout_toEndOf="@+id/button4" />


    <ImageButton
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="#EC5C2D"
        app:srcCompat="@drawable/wakeup"
        android:id="@+id/imageButton7"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:layout_below="@+id/button9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button9"
        android:layout_alignEnd="@+id/button9"
        android:layout_alignBottom="@+id/imageButton6" />

    <ImageButton
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="#EC5C2D"
        app:srcCompat="@drawable/sleep"
        android:id="@+id/imageButton6"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:layout_below="@+id/button9"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:layout_toLeftOf="@+id/button4"
        android:layout_toStartOf="@+id/button4"
        android:layout_alignBottom="@+id/imageButton10" />

    <ImageButton
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="#EC5C2D"
        app:srcCompat="@drawable/black_skull"
        android:id="@+id/imageButton10"

        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:layout_below="@+id/button4"
        android:layout_toRightOf="@+id/button5"
        android:layout_toEndOf="@+id/button5"
        android:layout_alignRight="@+id/button4"
        android:layout_alignBottom="@+id/button7" />

    <Button
        android:textColor="#E88137"
        android:text="Sit"
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button7"
        android:layout_below="@+id/imageButton2"
        android:layout_toRightOf="@+id/button4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:textColor="#E88137"
        android:backgroundTint="#111111"
        android:text="Lying Back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button2"
        android:layout_alignBaseline="@+id/button3"
        android:layout_alignBottom="@+id/button3"
        android:layout_toRightOf="@+id/button3" />

    <Button
        android:textColor="#E88137"
        android:backgroundTint="#111111"
        android:text="Crouch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button8"
        android:layout_below="@+id/imageView"
        android:layout_toRightOf="@+id/button6"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:textColor="#E88137"
        android:backgroundTint="#111111"
        android:text="Auto-Pilot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button10" />

    <Button
        android:textColor="#E88137"
        android:backgroundTint="#111111"
        android:text="AUTO-PILOT OFF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/button11" />

    <Button
        android:textColor="#E88137"
        android:backgroundTint="#111111"
        android:text="Message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button12"
        android:layout_below="@+id/imageButton7"
        android:layout_alignRight="@+id/imageButton7"
        android:layout_alignEnd="@+id/imageButton7" />

    <ImageButton
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="#EC5C2D"
        app:srcCompat="@drawable/microphone"
        android:id="@+id/imageButton9"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"

        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/button7"
        android:layout_alignBottom="@+id/button12"
        android:layout_alignLeft="@+id/imageButton8"
        android:layout_alignStart="@+id/imageButton8" />

    <ImageButton
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:tint="#EC5C2D"
        app:srcCompat="@drawable/hear"
        android:id="@+id/imageButton8"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_below="@+id/imageButton9"
        android:layout_alignBottom="@+id/imageButton4" />

    <ImageButton
        android:backgroundTint="#111111"
        android:layout_width="wrap_content"
        android:tint="#EC5C2D"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/rec"
        android:layout_alignLeft="@+id/button12"
        android:layout_alignStart="@+id/button12"
        android:id="@+id/imageButton11"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_below="@+id/button12"
        android:layout_alignBottom="@+id/imageButton3" />

    <Button
        android:textColor="#E88137"
        android:backgroundTint="#111111"
        android:text="Stand Zero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        android:layout_alignBaseline="@+id/button9"
        android:layout_alignBottom="@+id/button9"

        android:textSize="13sp"
        android:layout_toRightOf="@+id/button9"
        android:layout_toLeftOf="@+id/button4"
        android:layout_toStartOf="@+id/button4" />

</RelativeLayout>

